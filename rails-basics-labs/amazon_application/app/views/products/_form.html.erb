<%= form_with model: @product, local: true do |form| %>
    <div>
        <%= form.label :title %>
        <br>
        <%= form.text_field :title %>
    </div>
    <div>
        <%= form.label :description %>
        <br>
        <%= form.text_area :description, cols:50, rows:5 %>
    </div>
    <%= form.submit("create") %>

    
<% end %>